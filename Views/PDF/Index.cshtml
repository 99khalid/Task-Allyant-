@{
    ViewData["Title"] = "Upload PDF Files";
}
<h1>Upload PDF Files</h1>
<form asp-controller="PDF" asp-action="ProcessFiles" method="post" enctype="multipart/form-data">
    <label for="pdfFiles">Select PDF files to upload:</label><br />
    <input type="file" id="pdfFiles" name="pdfFiles" multiple accept=".pdf" /><br /><br />
    <button type="submit">Process Files</button>
</form>
